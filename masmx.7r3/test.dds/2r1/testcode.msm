tst_po_missing: DB 13,'Paper Out    '
tst_po_present: DB 13,'Paper Present'

TST_PaperOut:
	CALL	PRT_StartPrint
	CALL	LCD_Clear
        CALL    KBD_DisableKeyInts
TST_POloop:
	MOV	A,P7
	JNB	prt_paper_sense,TST_POpresent
        MOV	DPTR,#tst_po_missing
	JMP	TST_POdisplay
TST_POpresent:
	MOV	DPTR,#tst_po_present
TST_POdisplay:
	MOV	A,#0
	CALL	LCD_GotoXY
	CALL	LCD_DisplayStringCODE
        CALL	KBD_ScanKeyboard
        JZ	TST_POloop
        CJNE	A,#KBD_CANCEL,TST_POfeed
        CALL    KBD_EnableKeyInts
        CALL    testdelay
        CALL	KBD_FlushKeyboard
        CALL	PRT_EndPrint
        CLR     A
	RET
TST_POfeed:
	MOV	R7,#1
	CALL	PRT_LineFeed
	JMP	TST_POloop

;******************************************************************************

TST_PaperFeed:
	CALL	PRT_StartPrint
	CALL	LCD_Clear
	CALL	KBD_DisableKeyInts
TST_PFloop:
;***
	MOV	A,ADCON0
	ANL	A,#0C0h		; keep two bits nothing to do with ADC
	MOV	ADCON0,A
	MOV	ADCON1,#04h		; ADC7 = battery
	MOV	DAPR,#0
	NOP
	NOP
	NOP
	NOP
	NOP
TST_PFwait:
	JB	0DCh,TST_PFwait
	MOV	A,ADDAT
;        CALL    DBG_TxChar
;***
        CALL	KBD_ScanKeyboard
        JZ	TST_PFloop
        CJNE	A,#KBD_CANCEL,TST_PFfeed
        CALL	KBD_EnableKeyInts
        CALL    testdelay
        CALL	KBD_FlushKeyboard
        CALL	PRT_EndPrint
        CLR     A
	RET
TST_PFfeed:
	CLR	prt_paperout
	MOV	R7,#1
	CALL	PRT_LineFeed
	JMP	TST_PFloop

;******************************************************************************

TST_FormFeed:
	CLR	prt_paperout
        CALL	LED_Led1Off
	CALL	PRT_StartPrint
	CALL	PRT_FormFeed
        CALL	PRT_EndPrint
        CLR     A
        RET

;******************************************************************************

TST_ContinuousFormFeed:
	CLR	prt_paperout
        CALL	LED_Led1Off
        CALL	PRT_StartPrint
TST_CFFloop:
	CALL	KBD_ReadKey
        JNZ	TST_CFFdone
        CALL	PRT_FormFeed
        MOV     R0,#3
        CALL    delay100ms
        JNB	prt_paperout,TST_CFFloop
TST_CFFdone:
        CALL	PRT_EndPrint
        CLR     A
        RET

;******************************************************************************

tst_prtcutout: DB 255,21,0,0,0,0,21,'       ',3,'     ',3,'       '
TST_PrintCutout:
	CALL	PRT_StartPrint
	CALL	PRT_ClearBitmap
        MOV     A,#8
        CALL    PRT_SetBitmapLenSmall
        MOV	DPTR,#tst_prtcutout
        CALL	PRT_FormatCODEField
        MOV	DPTR,#prt_bitmap
        CALL	PRT_TransferLine
        CALL	PRT_Strobe

	CLR	prt_head_oe_port	; prepare to turn OE on
	MOV	CMH2,#0			; cause a compare match to make the
	MOV	CML2,#0			; OE turn on
	MOV	TH2,#0
	MOV	TL2,#0
	MOV	CMH2,#100		; set timeout for next compare
	MOV	CML2,#0
	SETB	prt_head_oe_port	; at compare, make line go high (OE off)

        MOV	R7,#100
        CALL	PRT_LineFeed
        CALL	PRT_EndPrint
        CLR     A
	RET

