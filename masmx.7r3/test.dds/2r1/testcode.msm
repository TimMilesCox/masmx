msg_fixedramtest:	DB 22,'Fixed Pattern RAM Test'
msg_testfill:		DB  5,'Fill '
msg_testcompare:		DB  8,'Compare '
msg_cycleramtest:	DB 23,'Cyclic Pattern RAM Test'
msg_testfillcycle:	DB 10,'Fill Cycle'
msg_testcomparecycle:	DB 13,'Compare Cycle'
msg_ramfail:		DB 23,'Fail Page xx Addr xxxxx'
msg_noramdetected:	DB 24,'No Offboard Ram Detected'
msg_rampassed:		DB 15,'RAM Test Passed'

tst_ramtestmenu:
	DB 4
	DB '=====RAM Test Menu===='
	DW 0
	DB 'Auto Ram Detect xxxxK '
	DW TST_AutoRAMDetect
	DB 'Fixed Pattern Test    '
	DW TST_FixedPatternAutoRamTest
	DB 'Cyclic Pattern Test   '
	DW TST_CyclicPatternAutoRamTest


TST_RAMTests:
	CALL	MNU_NewMenu
TST_RTagain:
	MOV	DPTR,#tst_ramtestmenu
	CALL	MNU_LoadMenuCODE

	MOV	DPTR,#sys_ramsize
	MOVX	A,@DPTR
	CALL	MTH_LoadOp1Acc
	MOV	A,#32
	CALL	MTH_LoadOp2Acc
	CALL	MTH_Multiply32by16
	MOV	DPTR,#buffer
	CALL	MTH_StoreLong
	MOV	DPSEL,#0
	MOV	DPTR,#buffer
	MOV	DPSEL,#1
	MOV	DPTR,#buffer+1024+41
	MOV	R5,#4
	CALL	NUM_NewFormatDecimal32

	CALL	MNU_SelectMenuOption
        JNB     ACC.7,TST_RTagain
	CLR	A
	RET

TST_CheckRamSize:
	CALL	SYS_DetectRAM
	MOV	DPTR,#sys_ramsize
	MOVX	A,@DPTR
	JNZ	TST_CRSok
	CALL	LCD_Clear
	MOV	DPTR,#msg_noramdetected
	CALL	LCD_DisplayStringCODE
	CALL	SND_Warning
	CALL	KBD_WaitKey
	CLR	A
TST_CRSok:
	RET

TST_AutoRAMDetect:
	CALL	SYS_DetectRAM
	CLR	A
	RET

TST_FixedPatternAutoRamTest:
	CALL	TST_CheckRamSize
	JZ	TST_FPRTAfail
	CALL	TST_FixedPatternRamTest
TST_FPRTAfail:
	CLR	A
	RET

TST_CyclicPatternAutoRamTest:
	CALL	TST_CheckRamSize
	JZ	TST_CPRTAfail
	CALL	TST_CyclicPatternRamTest
TST_CPRTAfail:
	CLR	A
	RET

TST_FixedPatternRamTest:	;A=pages to test
	PUSHACC
	CALL	LCD_Clear
	MOV	DPTR,#msg_fixedramtest
	CALL	LCD_DisplayStringCODE
	POP	ACC
	MOV	B,A
	MOV	A,#000h
	CALL	TST_FillRAM
	CALL	TST_CompareRam
	JZ	TST_FPRTfail
	MOV	A,#0FFh
	CALL	TST_FillRAM
	CALL	TST_CompareRam
	JZ	TST_FPRTfail
	MOV	A,#055h
	CALL	TST_FillRAM
	CALL	TST_CompareRam
	JZ	TST_FPRTFail
	MOV	A,#0AAh
	CALL	TST_FillRAM
	CALL	TST_CompareRam
	JZ	TST_FPRTFail
	JMP	TST_RAMPass
TST_FPRTfail:
	JMP	TST_RAMFail

TST_FillRAM:	;B=pages,A=pattern
	PUSH	B
	PUSH	ACC
	CALL	LCD_Clear2
	MOV	A,#64			; display "Fill" message
	CALL	LCD_GotoXY		;
	MOV	DPTR,#msg_testfill	;
	CALL	LCD_DisplayStringCODE	;
	POP	ACC
	PUSH	ACC
	SWAP	A
	ANL	A,#00Fh
	CALL	HexChar
	CALL	LCD_WriteData
	POP	ACC
	PUSH	ACC
	ANL	A,#00Fh
	CALL	HexChar
	CALL	LCD_WriteData
	POP	ACC
	POP	B
	PUSH	B
	MOV	R4,#0
	MOV	R7,B
TST_FillRAMpageloop:
	ANL	SB1data,#0E0h		; set page addressing
	PUSH	ACC			;
	MOV	A,R4			;
	ORL	SB1data,A		;
	POP	ACC			;
;	CALL	SBS_WriteSB1

	MOV	DPTR,#08000h
	MOV	B,#0
	MOV	R5,#127
	MOV	R6,#0
TST_FillRAMbyteloop:
	MOVX	@DPTR,A
	INC	DPTR
	DJNZ	R6,TST_FillRAMbyteloop
	DJNZ	R5,TST_FillRAMbyteloop
	INC	R4
	DJNZ	R7,TST_FillRAMpageloop
	POP	B
	RET

TST_CompareRAM:	;B=pages,A=pattern
	PUSH	B
	PUSH	ACC
	CALL	LCD_Clear2
	MOV	A,#64			; display "Compare" message
	CALL	LCD_GotoXY		;
	MOV	DPTR,#msg_testcompare	;
	CALL	LCD_DisplayStringCODE	;
	POP	ACC
	PUSH	ACC
	SWAP	A
	ANL	A,#00Fh
	CALL	HexChar
	CALL	LCD_WriteData
	POP	ACC
	PUSH	ACC
	ANL	A,#00Fh
	CALL	HexChar
	CALL	LCD_WriteData
	POP	ACC
	POP	B
	PUSH	B
	MOV	R4,#0
	MOV	R7,B
TST_CompareRAMpageloop:
	ANL	SB1data,#0E0h		; set page addressing
	PUSH	ACC			;
	MOV	A,R4			;
	ORL	SB1data,A		;
	POP	ACC			;
;	CALL	SBS_WriteSB1

	MOV	DPTR,#08000h
	MOV	B,#0
	MOV	R5,#127
	MOV	R6,#0
TST_CompareRAMbyteloop:
	MOV	B,A
	MOVX	A,@DPTR
	CJNE	A,B,TST_CRfail
	INC	DPTR
	DJNZ	R6,TST_CompareRAMbyteloop
	DJNZ	R5,TST_CompareRAMbyteloop
	INC	R4
	DJNZ	R7,TST_CompareRAMpageloop
	POP	B
	MOV	A,#1			; success
	RET
TST_CRfail:
	POP	B
	CLR	A			; fail
	RET

TST_CyclicPatternRamTest: ;A=pages to test
	PUSH	ACC
	CALL	LCD_Clear
	MOV	DPTR,#msg_cycleramtest
	CALL	LCD_DisplayStringCODE
	POP	ACC
	MOV	B,A
	CALL	TST_FillCycleRAM
	CALL	TST_CompareCycleRAM
	JZ	TST_CPRTfail
	JMP	TST_RAMPass
TST_CPRTfail:
	JMP	TST_RAMFail

TST_FillCycleRAM:	;B=pages
	PUSH	B
	CALL	LCD_Clear2
	MOV	A,#64			; display "Fill" message
	CALL	LCD_GotoXY		;
	MOV	DPTR,#msg_testfillcycle	;
	CALL	LCD_DisplayStringCODE	;
	POP	B
	PUSH	B
	MOV	R4,#0
	MOV	R7,B
TST_FCRpageloop:
	ANL	SB1data,#0E0h		; set page addressing
	PUSH	ACC			;
	MOV	A,R4			;
	ORL	SB1data,A		;
	POP	ACC			;
;	CALL	SBS_WriteSB1

	MOV	DPTR,#08000h
	MOV	A,#1
	MOV	B,#0
	MOV	R5,#127
	MOV	R6,#0
TST_FCRbyteloop:
	MOVX	@DPTR,A
	INC	DPTR
	INC	A
	CJNE	A,#14,TST_FCRok
	MOV	A,#1
TST_FCRok:
	DJNZ	R6,TST_FCRbyteloop
	DJNZ	R5,TST_FCRbyteloop
	INC	R4
	DJNZ	R7,TST_FCRpageloop
	POP	B
	RET

TST_CompareCycleRAM:	;B=pages
	PUSH	B
	CALL	LCD_Clear2
	MOV	A,#64			; display "Compare" message
	CALL	LCD_GotoXY		;
	MOV	DPTR,#msg_testcomparecycle	;
	CALL	LCD_DisplayStringCODE	;
	POP	B
	PUSH	B
	MOV	R4,#0
	MOV	R7,B
TST_CRpageloop:
	ANL	SB1data,#0E0h		; set page addressing
	PUSH	ACC			;
	MOV	A,R4			;
	ORL	SB1data,A		;
	POP	ACC			;
;	CALL	SBS_WriteSB1

	MOV	DPTR,#08000h
	MOV	B,#1
	MOV	R5,#127
	MOV	R6,#0
TST_CRbyteloop:
	MOVX	A,@DPTR
	CJNE	A,B,TST_CRCfail
	INC	DPTR
	MOV	A,B
	INC	A
	CJNE	A,#14,TST_CRnowrap
	MOV	A,#1
TST_CRnowrap:
	MOV	B,A
	DJNZ	R6,TST_CRbyteloop
	DJNZ	R5,TST_CRbyteloop
	INC	R4
	DJNZ	R7,TST_CRpageloop
	POP	B
	MOV	A,#1			; success
	RET
TST_CRCfail:
	POP	B
	CLR	A			; fail
	RET

TST_RAMPass:
	CALL	LCD_Clear2
	MOV	A,#64
	CALL	LCD_GotoXY
	MOV	DPTR,#msg_rampassed
	CALL	LCD_DisplayStringCODE
	CALL	KBD_WaitKey
	RET

TST_RAMFail:
	PUSH	DPH
	PUSH	DPL
	MOV	A,R4
	PUSH	ACC
	CALL	LCD_Clear2
	MOV	A,#64
	CALL	LCD_GotoXY
	MOV	DPTR,#msg_ramfail
	CALL	LCD_DisplayStringCODE
	POP	ACC
	MOV	B,A
	MOV	DPSEL,#1
	MOV	DPTR,#buffer
	MOV	R5,#2
	CALL	NUM_NewFormatDecimalB
	MOV	A,#64+10
	CALL	LCD_GotoXY
	MOV	DPTR,#buffer
	MOV	R7,#2
	CALL	LCD_DisplayStringXRAM
	MOV	A,#64+18
	CALL	LCD_GotoXY
	POP	DPL
	POP	DPH
	MOV	B,DPH
	MOV	A,DPL
	MOV	DPTR,#buffer
	MOVX	@DPTR,A
	MOV	A,B
	INC	DPTR
	MOVX	@DPTR,A
	MOV	DPSEL,#0
	MOV	DPTR,#buffer
	MOV	DPSEL,#1
	MOV	DPTR,#buffer+2
	MOV	R5,#5
	CALL	NUM_NewFormatDecimal16
	MOV	DPTR,#buffer+2
	MOV	R7,#5
	CALL	LCD_DisplayStringXRAM

	CALL	SND_Warning
	CALL	KBD_WaitKey
	RET


