	CLR	A
	RET
TST_SetTime:
	CALL	TIM_ChangeTime
	CLR	A
	RET

TST_TestRTC:
	CALL	LCD_Clear
TST_RTCloop:
	MOV	DPSEL,#0
	MOV	DPTR,#buffer

	MOV	R1,#RTC_SLAVE		; prepare to use I2C RTC
	MOV	DPSEL,#1			; read the hours
	MOV	DPTR,#04h		;
	CALL	I2C_Read8		;
	JNZ	TST_RTCfail		;
	MOV	A,B			;
	ANL	A,#03Fh			;
	MOV	DPSEL,#0
	CALL	BCDtoASCII

	MOV	A,#':'
	MOVX	@DPTR,A
	INC	DPTR

	MOV	DPSEL,#1			; read the minutes
	MOV	DPTR,#03 		;
	CALL	I2C_Read8		;
	JNZ	TST_RTCfail		;
	MOV	A,B			;
	ANL	A,#07Fh			;
	MOV	DPSEL,#0
	CALL	BCDtoASCII

	MOV	A,#':'
	MOVX	@DPTR,A
	INC	DPTR

	MOV	DPSEL,#1			; read the seconds
	MOV	DPTR,#02 		;
	CALL	I2C_Read8		;
	JNZ	TST_RTCfail		;
	MOV	A,B			;
	ANL	A,#07Fh			;
	MOV	DPSEL,#0
	CALL	BCDtoASCII

;	CALL	LCD_Clear
	MOV	A,#10
	CALL	LCD_GotoXY
	MOV	DPTR,#buffer
	MOV	R7,#8
	CALL	LCD_DisplayStringXRAM

	MOV	A,#0
	CALL	LCD_GotoXY
	CALL	TIM_GetDateTime
	MOV	DPSEL,#1
	MOV	DPTR,#buffer
	CALL	TIM_FormatDate
	MOV	DPTR,#buffer
	MOV	R7,#8
	CALL	LCD_DisplayStringXRAM

	CALL	KBD_ReadKey
	JZ	TST_RTCloop
	CLR	A
	RET

TST_RTCfail:
	CALL	SND_Warning
	MOV	A,#64
	CALL	LCD_GotoXY
	MOV	DPTR,#msg_rtc_fail
	CALL	LCD_DisplayStringCODE
	CALL	KBD_WaitKey
	CLR	A
	RET

