msg_ee_testmother:	DB 23,'Testing On-board EEPROM'
msg_ee_testpriceplug:	DB 24,'Testing PricePlug EEPROM'
msg_ee_pass:		DB 13,'EEPROM Passed'
msg_ee_fail:		DB 13,'EEPROM Failed'

TST_EE:
	CALL	MNU_NewMenu
TST_EEagain:
	MOV	DPTR,#tst_eetestmenu
	CALL	MNU_LoadMenuCODE
	CALL	MNU_SelectMenuOption
        JNB     ACC.7,TST_EEagain
	CLR	A
	RET

TST_EEMotherboard:
	CALL	LCD_Clear
	MOV	DPTR,#msg_ee_testmother
	CALL	LCD_DisplayStringCODE
	CLR	F0
	SETB	F1
	MOV	R1,#EE_SLAVE
	MOV	DPTR,#255
	JMP	TST_EEGeneric

TST_EEpass:
	MOV	A,#64
	CALL	LCD_GotoXY
	MOV	DPTR,#msg_ee_pass
        CALL	SYS_PricePlugPowerOff
	CALL	LCD_DisplayStringCODE
	CALL	KBD_WaitKey
	CLR	A
	RET

TST_EEfail:
	MOV	A,#64
	CALL	LCD_GotoXY
	MOV	DPTR,#msg_ee_fail
	CALL	LCD_DisplayStringCODE
        CALL	SYS_PricePlugPowerOff
	CALL	SND_Warning
	CALL	KBD_WaitKey
	CLR	A
	RET

TST_EEPricePlug:
	CALL	SYS_PricePlugPowerOn
	MOV	R1,#PPG_EESLAVE
	CALL	LCD_Clear
	MOV	DPTR,#msg_ee_testpriceplug
	CALL	LCD_DisplayStringCODE
	SETB	F0
	CLR	F1
	MOV	DPTR,#2047
TST_EEGeneric:
	MOV	B,#0AAh
	CALL	I2C_Write
	CALL	I2C_Read
	JNZ	TST_EEfail
	MOV	A,B
	CJNE	A,#0AAh,TST_EEfail
	MOV	B,#055h
	CALL	I2C_Write
	CALL	I2C_Read
	JNZ	TST_EEfail
	MOV	A,B
	CJNE	A,#055h,TST_EEfail
	JMP	TST_EEpass
	RET

