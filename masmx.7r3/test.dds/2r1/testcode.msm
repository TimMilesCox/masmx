	CALL	SND_Warning
        CALL	KBD_WaitKey

; pull down rows of 21 bytes into prt_bitmap, then print it

        MOV     A,#0
        MOV     B,#1
        CALL    PRT_SetBitmapLen
        CALL	PRT_ClearBitmap
	MOV	DPTR,#prt_bitmap

TST_Grxloop1:
        MOV	R7,#21
        PUSHDPH
	PUSHDPL
