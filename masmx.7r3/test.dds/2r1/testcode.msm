tst_octavetable: DB 124,110,98,93,83,73,65,62
tst_msg_organ: DB 21,'Organ Mode - Play Me!'

TST_Organ:
	CALL	LCD_Clear
        MOV	DPTR,#tst_msg_organ
        CALL	LCD_DisplayStringCODE
TST_Oloop:
	CALL	KBD_ScanKeyboard
        JZ	TST_Onosound
        CJNE	A,#KBD_CANCEL,TST_Okey
        CLR	A
        RET
TST_Okey:
        CLR	C
        SUBB	A,#11
        MOV	DPTR,#tst_octavetable
        MOVC	A,@A+DPTR
        MOV	B,A
        CALL	SND_SoundOn
	JMP	TST_Oloop
TST_Onosound:
	CALL	SND_SoundOff
        JMP	TST_Oloop

;************************************************

notes: DB 124,98,110,93,73,83,65,62
lcd_test_sound: DB 10,'Sound Test'

TST_Sound1:
	MOV	R2,#255
        MOV	A,#0
        MOV	B,#255
DT_FDloop:
	CALL	SND_SoundOn
        MOV     R0,#30
        CALL    delay100us
        XCH	A,B
	CALL	SND_SoundOn
        MOV     R0,#30
        CALL    delay100us
        XCH     A,B
        INC     A
        DEC     B
        DJNZ	R2,DT_FDloop
        CALL	SND_SoundOff
	RET

TST_WierdSound:
	CALL	LCD_Clear
        MOV	A,#0
        CALL	LCD_GotoXY
        MOV	DPTR,#lcd_test_sound
        CALL	LCD_DisplayStringCODE

	MOV	B,#50
TST_Sloop1:
        CALL	SND_SoundOn
        MOV     R0,#20
        CALL    delay100us
        DEC     B
        MOV     A,B
        JNZ     TST_Sloop1

	MOV	B,#50
DT_FAloop:
        CALL	SND_SoundOn
        MOV     R0,#50
        CALL    delay100us
        DJNZ	B,DT_FAloop

	MOV	R2,#255
DT_FBloop:
	CLR	C
	MOV	A,#255
        SUBB	A,R2
        MOV	B,A
        CALL	SND_SoundOn
        MOV     R0,#10
        CALL    delay100us
        DJNZ	R2,DT_FBloop

	MOV	R2,#255
        MOV	A,#0
        MOV	B,#255
DT_FCloop:
	CALL	SND_SoundOn
        MOV     R0,#5
        CALL    delay100us
        XCH	A,B
	CALL	SND_SoundOn
        MOV     R0,#5
        CALL    delay100us
        XCH     A,B
        INC     A
        DEC     B
        DJNZ	R2,DT_FCloop

        CALL	TST_Sound1

	MOV	R2,#128
        MOV	A,#0
        MOV	B,#255
DT_FEloop:
	CALL	SND_SoundOn
        MOV     R0,#20
        CALL    delay100us
        XCH	A,B
	CALL	SND_SoundOn
        MOV     R0,#20
        CALL    delay100us
        XCH     A,B
        INC     A
        INC     A
        DEC     B
        DEC     B
        DEC     B
        DEC     B
        DJNZ	R2,DT_FEloop

	MOV	R2,#128
        MOV	A,#0
        MOV	B,#255
DT_FGloop:
	CALL	SND_SoundOn
        MOV     R0,#50
        CALL    delay100us
        XCH	A,B
	CALL	SND_SoundOn
        MOV     R0,#50
        CALL    delay100us
        XCH     A,B
        INC     A
        DEC     B
        DJNZ	R2,DT_FGloop

	MOV	DPTR,#notes
        MOV	R3,#8
DT_FHloop:
	CLR	A
	MOVC	A,@A+DPTR
        MOV	B,A
        CALL	SND_SoundOn
        MOV	R0,#1
        CALL	delay100ms
        INC	DPTR
        DJNZ	R3,DT_FHloop
        CALL	SND_SoundOff
        CALL    SND_Warning
	CLR	A
	RET

