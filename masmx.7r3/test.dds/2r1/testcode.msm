	MOV	DAPR,#0
	NOP
	NOP
	NOP
	NOP
	NOP
TST_DSMloop2:
	JB	0DCh,TST_DSMloop2
	MOV	A,ADDAT
        PUSH    ACC
;        CALL    DBG_TxChar
