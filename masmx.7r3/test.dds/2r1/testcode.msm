	CALL	LCD_DisplayStringCODE
	CALL	PRT_StartPrint
TST_STloop:
	CALL	KBD_WaitKey
	MOV     DPTR,#testkeytable
	DEC     A
	RL      A
	JMP     @A+DPTR
testkeytable:
	AJMP    TST_Key0
	AJMP    TST_Key1
	AJMP    TST_Key2
	AJMP    TST_Key3
	AJMP    TST_Key4
	AJMP    TST_Key5
	AJMP    TST_Key6
	AJMP    TST_Key7
	AJMP    TST_Key8
	AJMP    TST_Key9
	AJMP    TST_KeyA
	AJMP    TST_KeyB
	AJMP    TST_KeyC
	AJMP    TST_KeyD
	AJMP    TST_KeyE
	AJMP    TST_KeyF
	AJMP    TST_KeyG
	AJMP    TST_KeyH
	AJMP    TST_KeyCancel
	AJMP    TST_KeyOk
TST_Key0:
	MOV	R7,#255
TST_K0loop:
	CALL	PRT_GeneratePulse
	CALL	PRT_CheckPaper
        MOV	R0,prt_stepdelay
	CALL	delay100us
	DJNZ	R7,TST_K0loop
	CALL	PRT_StopMotor
	JMP	TST_STloop
TST_Key1:
TST_Key2:
TST_Key3:
TST_Key4:
TST_Key5:
TST_Key6:
TST_Key7:
TST_Key8:
TST_Key9:
TST_KeyA:
	INC	prt_stepdelay
        JMP	TST_STloop
TST_KeyB:
	DEC	prt_stepdelay
        JMP	TST_STloop
TST_KeyC:
	INC	prt_stepdelay2
        JMP	TST_STloop
TST_KeyD:
	DEC	prt_stepdelay2
        JMP	TST_STloop
TST_KeyE:
