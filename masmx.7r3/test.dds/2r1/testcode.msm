	DB 'Set Time              '
	DW TST_SetTime

;****

TST_RealTimeClock:
	CALL	MNU_NewMenu
TST_RTCagain:
	MOV	DPTR,#tst_rtctestmenu
	CALL	MNU_LoadMenuCODE
	CALL	MNU_SelectMenuOption
        JNB     ACC.7,TST_RTCagain
	CLR	A
