	MOV	B,#COM_COM0
        MOV	R5,#10
        CALL	COM_RxCharTimeout
        JNC	TST_Gprt
        MOVX	@DPTR,A
        INC	DPTR
        DJNZ	R7,TST_Grxloop2
        POP	DPL
        POP	DPH
        MOV	A,#32
        CALL	AddAtoDPTR
        JMP	TST_Grxloop1

