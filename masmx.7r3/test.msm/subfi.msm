p*      $proc
        +       p(1,1)
        +       p(1,1:1)
        +       p(1,1:2)
        q       p(1,1), p(1,1:1), p(1,1:2)
        $end

q*      $proc
        +       q(1,1), q(1,2), q(1,3)
        $end

x       $equ    3
y       $equ    4
x:y     $equ    5

        p       x:y
