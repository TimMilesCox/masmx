top*    $proc
        middle   top(1, 1)
        $end
middle* $proc
        +       middle(1, *1), middle(1, 1), middle(1, 2)
        +       middle(1, #1), middle(1, 1), middle(1, 3)
        lower   middle(1, *1), middle(1, 1), middle(1, *4)
        lower   middle(1, #1), middle(1, 1), middle(1, 2)
        lower   middle(1, #1), middle(1, 1), middle(1, 3)
        $end
lower*  $proc
        +       lower(1, #1), lower(1, 2), lower(1, *3)
        $end
        
	$plist	20

        top     7
        top     *6, 1
        top     #5,,3
        top     4, ,,,*4
